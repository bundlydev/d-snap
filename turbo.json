{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": [
        "generate",
        "canister:local:create",
        "stop",
        "serve",
        "deploy:local"
      ],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["generate", "canister:local:create", "^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "start": {},
    "lint": {
      "dependsOn": ["^lint"]
    },
    "serve": {
      "outputs": [".dfx"]
    },
    "stop": {},
    "generate": {
      "outputs": ["src/declarations"]
    },
    "canister:local:create": {},
    "deploy:local": {
      "dependsOn": ["build"],
      "outputs": [".dfx/local"]
    },
    "deploy:ic": {
      "dependsOn": ["build"]
    },
    "clean": {
      "dependsOn": ["stop"]
    }
  }
}
